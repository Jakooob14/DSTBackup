        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:DSTBackup"
        mc:Ignorable="d"
        Title="DSTBackup Demo" Height="200" Width="300" ResizeMode="CanMinimize">
    <StackPanel x:Name="MainPanel" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Button x:Name="StartDemoButton" Height="119" Width="300" Click="StartDemoButton_Click">Start Demo</Button>
    </StackPanel>
</Window>
